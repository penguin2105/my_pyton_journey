# Выводим список покупок по категориям
print("Список покупок:")
current_category = None

for item in sorted(shopping_list, key=lambda x: x["category"]):
    if item["category"] != current_category:
        print(f"\n{item['category'].title()}:")
        current_category = item["category"]
    print(f"  - {item['name']} ({item['quantity']} шт.)")

# Подсчёт общего количества
total_quantity = sum(item["quantity"] for item in shopping_list)
print(f"\nОбщее количество продуктов: {total_quantity} шт.")
